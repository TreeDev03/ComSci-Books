<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.comsci">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_comisci"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_comisci"
        android:supportsRtl="true"
        android:theme="@style/Theme.Sample">
        <activity
            android:name="com.example.comsci.FavoriteActivity"
            android:exported="true" />
        <activity
            android:name="com.example.comsci.CurrentActivity"
            android:exported="true" />
        <activity
            android:name="com.example.comsci.AlreadyReadBookActivity"
            android:exported="true" />
        <activity
            android:name="com.example.comsci.BookActivity"
            android:exported="true" />
        <activity
            android:name="com.example.comsci.AllBooksActivity"
            android:exported="true" />
        <activity
            android:name="com.example.comsci.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>